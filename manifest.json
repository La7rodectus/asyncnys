{
  "manifest_version": 2,
  "name": "asyncnys",
  "version": "0.0.1",
  "description": "Sync videos",
  "icons": {
    "128": "./resources/icon_16.png"
  },
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": ["./scr/content.js"]
    }
  ],
  "browser_action": {
    "default_icon": "./resources/icon_16.png"
  },
  "background": {
    "scripts": ["./scr/background.js"]
  }
}
